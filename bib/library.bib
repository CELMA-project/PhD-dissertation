doi = {10.5194/angeo-33-875-2015},
file = {:home/mmag/Documents/Utdanning/Danmarks{\_}Tekniske{\_}Universitet/Articles/Annales Geophysicae/P{\'{e}}cseli/P{\'{e}}cseli - 2015 - Annales Geophysicae - Spectral properties of electrostatic drift wave turbulence in the laboratory and the ionosphere.pdf:pdf},
issn = {14320576},
journal = {Annales Geophysicae},
keywords = {electromagnetics,plasmas},
