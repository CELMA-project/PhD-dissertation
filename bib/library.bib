keywords = {,anomalous transport,edge turbulence,fluid equations,gradient-driven,magnetized nonuniform plasma,particle-transport,tearing mode,texas experimental tokamak,toroidal plasmas,wave turbulence},
number = {10},
pages = {1635--1668},
title = {{Three-dimensional computation of drift Alfven turbulence}},
url = {{\textless}Go to ISI{\textgreater}://WOS:A1997YF17200010 {\{}{\textless}{\}}Go to ISI{\{}{\textgreater}{\}}://WOS:A1997YF17200010},
