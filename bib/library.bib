@article{Rasmussen2006a,
abstract = {The spontaneous generation of large-scale flows by the rectification of small-scale turbulent fluctuations is of great importance both in geophysical flows and in magnetically confined plasmas. These flows regulate the turbulence and may set up effective transport barriers. In the present contribution the generation of zonal flows will be illustrated in a simple fluid experiment performed in a rotating container with radial symmetric bottom topography. An effective mixing that homogenizes the potential vorticity in the fluid layer will lead to the replacement of the high-potential vorticity near the centre with low potential vorticity from the outside, which will imply the formation of a large-scale flow. The experimental results are supported by direct numerical solutions of the quasi-geostrophic vorticity equation in the $\beta$-plane approximation modelling the experimental situation. The analogy to large-scale flow generation in drift-wave turbulence dynamics in magnetized plasma is briefly discussed.},
author = {Rasmussen, J. J. and Garcia, O. E. and Naulin, V. and Nielsen, A. H. and Stenum, B. and van Bokhoven, L. J. A. and Delaux, S.},
doi = {10.1088/0031-8949/2006/T122/009},
file = {:home/mmag/Desktop/Danmarks{\_}Tekniske{\_}Universitet/Articles/Physica Scripta/Rasmussen et al/Rasmussen et al. - 2006 - Physica Scripta - Generation of zonal flows in rotating fluids and magnetized plasmas.pdf:pdf},
issn = {0031-8949},
journal = {Physica Scripta},
keywords = {Hasegawa Mima},
mendeley-tags = {Hasegawa Mima},
pages = {44--51},
title = {{Generation of zonal flows in rotating fluids and magnetized plasmas}},
volume = {T122},
year = {2006}
}
